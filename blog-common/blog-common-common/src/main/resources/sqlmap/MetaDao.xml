<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zeroq6.blog.common.dao.MetaDao">

    <sql id="TABLE_T"><![CDATA[meta t]]></sql>

    <sql id="TABLE"><![CDATA[meta]]></sql>

    <sql id="SELECT_COLUMN_LIST">
        <![CDATA[
            t.id AS id, /*主键*/
            t.data_type AS dataType, /*数据类型，1，文章*/
            t.data_id AS dataId, /*数据主键*/
            t.data_attribute AS dataAttribute, /*数据属性*/
            t.data_value AS dataValue, /*数据值*/
            t.remark AS remark, /*备注*/
            t.modified_time AS modifiedTime, /*修改时间*/
            t.created_time AS createdTime, /*创建时间*/
            t.yn AS yn /*是否有效，1，有效，0，无效*/
        ]]>
    </sql>

    <sql id="WHERE_CLAUSE_SELECT">
        WHERE t.yn = 1
        <if test="id != null">
            <![CDATA[AND t.id = #{id}]]> /*主键*/
        </if>
        <if test="dataType != null">
            <![CDATA[AND t.data_type = #{dataType}]]> /*数据类型，1，文章*/
        </if>
        <if test="dataId != null">
            <![CDATA[AND t.data_id = #{dataId}]]> /*数据主键*/
        </if>
        <if test="dataAttribute != null and dataAttribute.trim() != ''">
            <![CDATA[AND t.data_attribute = #{dataAttribute}]]> /*数据属性*/
        </if>
        <if test="dataValue != null and dataValue.trim() != ''">
            <![CDATA[AND t.data_value = #{dataValue}]]> /*数据值*/
        </if>
        <if test="remark != null and remark.trim() != ''">
            <![CDATA[AND t.remark = #{remark}]]> /*备注*/
        </if>
        <if test="modifiedTime != null">
            <![CDATA[AND t.modified_time = #{modifiedTime}]]> /*修改时间*/
        </if>
        <if test="createdTime != null">
            <![CDATA[AND t.created_time = #{createdTime}]]> /*创建时间*/
        </if>
        <if test="yn != null">
            <![CDATA[AND t.yn = #{yn}]]> /*是否有效，1，有效，0，无效*/
        </if>
        <if test="extendMap!=null">
            <if test="extendMap.dataAttributeLike!= null and extendMap.dataAttributeLike.trim() != ''">
                <![CDATA[AND t.data_attribute like concat('%',#{extendMap.dataAttributeLike},'%' )]]> /*数据属性*/
            </if>
            <if test="extendMap.dataValueLike!= null and extendMap.dataValueLike.trim() != ''">
                <![CDATA[AND t.data_value like concat('%',#{extendMap.dataValueLike},'%' )]]> /*数据值*/
            </if>
            <if test="extendMap.remarkLike!= null and extendMap.remarkLike.trim() != ''">
                <![CDATA[AND t.remark like concat('%',#{extendMap.remarkLike},'%' )]]> /*备注*/
            </if>
            <if test="extendMap.modifiedTimeBegin!= null and extendMap.modifiedTimeBegin != ''">
                <![CDATA[AND t.modified_time >= STR_TO_DATE(#{extendMap.modifiedTimeBegin},'%Y-%m-%d %H:%i:%s')]]> /*修改时间开始*/
            </if>
            <if test="extendMap.modifiedTimeEnd!= null and extendMap.modifiedTimeEnd != ''">
                <![CDATA[AND t.modified_time <= STR_TO_DATE(#{extendMap.modifiedTimeEnd},'%Y-%m-%d %H:%i:%s')]]> /*修改时间结束*/
            </if>
            <if test="extendMap.createdTimeBegin!= null and extendMap.createdTimeBegin != ''">
                <![CDATA[AND t.created_time >= STR_TO_DATE(#{extendMap.createdTimeBegin},'%Y-%m-%d %H:%i:%s')]]> /*创建时间开始*/
            </if>
            <if test="extendMap.createdTimeEnd!= null and extendMap.createdTimeEnd != ''">
                <![CDATA[AND t.created_time <= STR_TO_DATE(#{extendMap.createdTimeEnd},'%Y-%m-%d %H:%i:%s')]]> /*创建时间结束*/
            </if>
            <if test="extendMap.idIn!= null">
                <![CDATA[AND t.id in ]]>
                <foreach item="item" index="index" collection="extendMap.idIn" open="(" separator="," close=")">
                  #{item}
                </foreach>
            </if>
            <if test="extendMap.dataTypeIn!= null">
                <![CDATA[AND t.data_type in ]]>
                <foreach item="item" index="index" collection="extendMap.dataTypeIn" open="(" separator="," close=")">
                  #{item}
                </foreach>
            </if>
            <!--自定义开始 -->

            <!--自定义结束 -->
        </if>
    </sql>

    <sql id="WHERE_CLAUSE_UPDATE">
        <!--如果extendMap或where为空，则mybatis解析表达式空指针，执行不了-->
        WHERE t.yn = 1
        <if test="extendMap.where.id!= null">
            <![CDATA[AND t.id = #{extendMap.where.id}]]> /*主键*/
        </if>
        <if test="extendMap.where.dataType!= null">
            <![CDATA[AND t.data_type = #{extendMap.where.dataType}]]> /*数据类型，1，文章*/
        </if>
        <if test="extendMap.where.dataId!= null">
            <![CDATA[AND t.data_id = #{extendMap.where.dataId}]]> /*数据主键*/
        </if>
        <if test="extendMap.where.dataAttribute!= null and extendMap.where.dataAttribute.trim() != ''">
            <![CDATA[AND t.data_attribute = #{extendMap.where.dataAttribute}]]> /*数据属性*/
        </if>
        <if test="extendMap.where.dataValue!= null and extendMap.where.dataValue.trim() != ''">
            <![CDATA[AND t.data_value = #{extendMap.where.dataValue}]]> /*数据值*/
        </if>
        <if test="extendMap.where.remark!= null and extendMap.where.remark.trim() != ''">
            <![CDATA[AND t.remark = #{extendMap.where.remark}]]> /*备注*/
        </if>
        <if test="extendMap.where.modifiedTime!= null">
            <![CDATA[AND t.modified_time = #{extendMap.where.modifiedTime}]]> /*修改时间*/
        </if>
        <if test="extendMap.where.createdTime!= null">
            <![CDATA[AND t.created_time = #{extendMap.where.createdTime}]]> /*创建时间*/
        </if>
        <if test="extendMap.where.yn!= null">
            <![CDATA[AND t.yn = #{extendMap.where.yn}]]> /*是否有效，1，有效，0，无效*/
        </if>
    </sql>

    <!-- 更新列字段,只要不为NULL则更新,除开主键列 -->
    <sql id="UPDATE_COLUMN_SET">
        <set>
            <if test="dataType!= null">
                <![CDATA[ t.data_type = #{dataType}]]> /*数据类型，1，文章*/,
            </if>
            <if test="dataId!= null">
                <![CDATA[ t.data_id = #{dataId}]]> /*数据主键*/,
            </if>
            <if test="dataAttribute!= null and dataAttribute.trim() != ''">
                <![CDATA[ t.data_attribute = #{dataAttribute}]]> /*数据属性*/,
            </if>
            <if test="dataValue!= null and dataValue.trim() != ''">
                <![CDATA[ t.data_value = #{dataValue}]]> /*数据值*/,
            </if>
            <if test="remark!= null and remark.trim() != ''">
                <![CDATA[ t.remark = #{remark}]]> /*备注*/,
            </if>
            <if test="modifiedTime!= null">
                <![CDATA[ t.modified_time = #{modifiedTime}]]> /*修改时间*/,
            </if>
            <if test="createdTime!= null">
                <![CDATA[ t.created_time = #{createdTime}]]> /*创建时间*/,
            </if>
            <if test="yn!= null">
                <![CDATA[ t.yn = #{yn}]]> /*是否有效，1，有效，0，无效*/,
            </if>
        </set>
    </sql>


    <!-- 智能排序与分页 -->
    <sql id="QUERY_ORDER_LIMIT_CONDITION">
        <if test="orderField != null and orderField != '' and orderFieldType != null and orderFieldType != ''"><![CDATA[ORDER BY ${orderField} ${orderFieldType}]]></if>
        <if test="startIndex != null and startIndex &gt;= 0 and pageSize != null and pageSize &gt; 0"><![CDATA[LIMIT #{startIndex},#{pageSize}]]></if>
    </sql>


    <sql id="INSERT_COLUMN"><if test="dataType != null ">data_type,</if><if test="dataId != null ">data_id,</if><if test="dataAttribute != null and dataAttribute.trim() != ''">data_attribute,</if><if test="dataValue != null and dataValue.trim() != ''">data_value,</if><if test="remark != null and remark.trim() != ''">remark,</if><if test="modifiedTime != null ">modified_time,</if><if test="createdTime != null ">created_time,</if><if test="yn != null ">yn</if></sql>

    <sql id="INSERT_COLUMN_VALUES"><if test="dataType != null ">#{dataType},</if><if test="dataId != null ">#{dataId},</if><if test="dataAttribute != null and dataAttribute.trim() != ''">#{dataAttribute},</if><if test="dataValue != null and dataValue.trim() != ''">#{dataValue},</if><if test="remark != null and remark.trim() != ''">#{remark},</if><if test="modifiedTime != null ">#{modifiedTime},</if><if test="createdTime != null ">#{createdTime},</if><if test="yn != null ">#{yn}</if></sql>

    <insert id="insert" parameterType="com.zeroq6.blog.common.domain.MetaDomain">
        INSERT INTO <include refid="TABLE"/>
        (
          <include refid="INSERT_COLUMN"/>
        )
        VALUES
        (
        <include refid="INSERT_COLUMN_VALUES"/>
        )
    </insert>

    <insert id="insertFillingId" parameterType="com.zeroq6.blog.common.domain.MetaDomain" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO <include refid="TABLE"/>
        (
        <include refid="INSERT_COLUMN"/>
        )
        VALUES
        (
        <include refid="INSERT_COLUMN_VALUES"/>
        )
    </insert>

    <insert id="insertBatch" parameterType="java.util.List">
        INSERT INTO <include refid="TABLE"/>
        (
        data_type,data_id,data_attribute,data_value,remark,modified_time,created_time,yn
        )
        VALUES
        <foreach collection="list" index="index" item="item" separator=",">
            (#{item.dataType},#{item.dataId},#{item.dataAttribute},#{item.dataValue},#{item.remark},#{item.modifiedTime},#{item.createdTime},#{item.yn})
        </foreach>

    </insert>

    <select id="selectByKey" parameterType="long" resultType="com.zeroq6.blog.common.domain.MetaDomain">
        SELECT
        <include refid="SELECT_COLUMN_LIST"/>
        FROM
        <include refid="TABLE_T"/>
        WHERE t.yn = 1 AND t.id = #{id}
    </select>

    <select id="selectList" parameterType="com.zeroq6.blog.common.domain.MetaDomain" resultType="com.zeroq6.blog.common.domain.MetaDomain">
        SELECT
        <include refid="SELECT_COLUMN_LIST"/>
        FROM
        <include refid="TABLE_T"/>
        <include refid="WHERE_CLAUSE_SELECT"/>
        <include refid="QUERY_ORDER_LIMIT_CONDITION"/>
    </select>

    <select id="selectListCount" parameterType="com.zeroq6.blog.common.domain.MetaDomain" resultType="java.lang.Integer">
        SELECT
        count(1)
        FROM
        <include refid="TABLE_T"/>
        <include refid="WHERE_CLAUSE_SELECT"/>
    </select>

    <update id="updateByKey" parameterType="com.zeroq6.blog.common.domain.MetaDomain" >
        UPDATE <include refid="TABLE_T"/>
        <include refid="UPDATE_COLUMN_SET"/>
        WHERE t.yn = 1 AND t.id = #{id}
    </update>

    <update id="updateByCondition" parameterType="com.zeroq6.blog.common.domain.MetaDomain">
        UPDATE <include refid="TABLE_T"/>
        <include refid="UPDATE_COLUMN_SET"/>
        <include refid="WHERE_CLAUSE_UPDATE"/>
    </update>

    <update id="disableByKey" parameterType="long">
        UPDATE <include refid="TABLE_T"/>
        SET t.yn = 0
        WHERE t.yn = 1 AND t.id = #{_parameter}
    </update>

    <update id="disableByCondition" parameterType="com.zeroq6.blog.common.domain.MetaDomain">
        UPDATE <include refid="TABLE_T"/>
        SET t.yn = 0
        <include refid="WHERE_CLAUSE_SELECT"/> AND t.yn = 1
    </update>


    <!--系统生成结束,请勿修改,重新生成会覆盖 -->
    <!--自定义开始 -->

    <!--自定义结束 -->
</mapper>
