<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title>$!{title}</title>
    <subtitle>$!{subtitle}</subtitle>
    <link href="$!{atom.uri}" rel="self"/>
    <link href="$!{url}"/>
    <updated>$!{updated}</updated>
    <id>$!{url}</id>
    <author>
        <name>$!{atom.author}</name>
    </author>
    <generator uri="$!{atom.generator.url}">$!{atom.generator.text}</generator>
#foreach($item in $postDomainList)
    <entry>
        <title>$!{item.title}</title>
        <link href="$!{item.extendMap.url}"/>
        <id>$!{item.extendMap.url}</id>
        <published>$!{item.extendMap.published}</published>
        <updated>$!{item.extendMap.updated}</updated>

        <content type="html"><![CDATA[$!{item.extendMap.content}]]></content>

        <summary type="html">
            <![CDATA[<p>$!{item.extendMap.summary}</p>]]>
        </summary>
#foreach($entry in $item.extendMap.tags)
        <category term="$!{entry.key}" scheme="$!{entry.value}"/>
#end
#foreach($entry in $item.extendMap.category)
        <category term="$!{entry.key}" scheme="$!{entry.value}"/>
#end
    </entry>
#end
</feed>
