#set($layout = 'admin/layout/layout.vm')
                <link href="//cdn.bootcss.com/dropzone/4.3.0/min/dropzone.min.css" rel="stylesheet">
                <style>
                    #dropzone {
                        margin-bottom: 3rem;
                    }
                    .dropzone {
                        border: 2px dashed #0087F7;
                        border-radius: 5px;
                        background: white;
                    }
                    .dropzone .dz-message {
                        font-weight: 400;
                    }
                    .dropzone .dz-message .note {
                        font-size: 0.8em;
                        font-weight: 200;
                        display: block;
                        margin-top: 1.4rem;
                    }
                    .attach-img {
                        width: 100px;
                        height: 100px;
                        border-radius: 10px;
                        box-shadow: 0px 0px 8px #333;
                    }
                    .attach-text {
                        font-size: 12px;
                        font-weight: 300;
                    }
                    .attach-img:hover {
                        background-color: #f9f9f9;
                    }
                </style>
